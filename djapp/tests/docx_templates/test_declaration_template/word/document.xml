				</w:rPr>
			</w:pPr>
			<w:r w:rsidRPr="00507591">
				<w:rPr>
					<w:rFonts w:asciiTheme="minorHAnsi" w:hAnsiTheme="minorHAnsi" w:cstheme="minorHAnsi"/>
					<w:i/>
					<w:iCs/>
					<w:vertAlign w:val="superscript"/>
				</w:rPr>
				<w:t>(указывается либо "да, согласен(на)", либо "нет, не согласен(на)")</w:t>
			</w:r>
		</w:p>
		<w:p w14:paraId="21633736" w14:textId="77777777" w:rsidR="00FE6E9F" w:rsidRPr="00507591" w:rsidRDefault="00FE6E9F" w:rsidP="005579E8">
			<w:pPr>
				<w:ind w:firstLine="0"/>
