		<w:p w14:paraId="57D4FFB9" w14:textId="67F2B4DA" w:rsidR="00FE6E9F" w:rsidRPr="00507591" w:rsidRDefault="00FA593C" w:rsidP="00507591">
			<w:pPr>
				<w:pStyle w:val="a8"/>
